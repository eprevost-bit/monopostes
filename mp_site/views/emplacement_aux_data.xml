<odoo>

    <record id="view_focus_number_tree" model="ir.ui.view">
        <field name="name">focus.number.tree</field>
        <field name="model">mp.emplacement.focus.number</field>
        <field name="arch" type="xml">
            <list string="Nº Focos">
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="view_focus_number_form" model="ir.ui.view">
        <field name="name">focus.number.form</field>
        <field name="model">mp.emplacement.focus.number</field>
        <field name="arch" type="xml">
            <form string="Nº Focos">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_focus_number" model="ir.actions.act_window">
        <field name="name">Nº Focos</field>
        <field name="res_model">mp.emplacement.focus.number</field>
        <field name="view_mode">list,form</field>
    </record>

    <menuitem id="menu_focus_number_action"
              name="Nº Focos"
              parent="mp_site_menu_config_root"
              action="action_focus_number"/>




